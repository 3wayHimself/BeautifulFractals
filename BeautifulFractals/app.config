<?xml version="1.0"?>
<configuration>
  <configSections>
      <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
          <section name="TAlex.BeautifulFractals.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
      </sectionGroup>
      <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
          <section name="TAlex.BeautifulFractals.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
      </sectionGroup>
  </configSections>

  <appSettings>
    <add key="TAlexConfiguration_ErrorReportUrl" value="http://talex-soft.com/api/ErrorReporting/Send"/>
  </appSettings>

  <userSettings>
      <TAlex.BeautifulFractals.Properties.Settings>
          <setting name="RandomOrder" serializeAs="String">
              <value>True</value>
          </setting>
          <setting name="ExitOnMouseMove" serializeAs="String">
              <value>True</value>
          </setting>
          <setting name="Delay" serializeAs="String">
              <value>00:00:03</value>
          </setting>
          <setting name="ShowFractalCaptions" serializeAs="String">
              <value>True</value>
          </setting>
          <setting name="PrimaryBackColor" serializeAs="String">
              <value>#ff1e90ff</value>
          </setting>
          <setting name="SecondaryBackColor" serializeAs="String">
              <value>#ff000000</value>
          </setting>
          <setting name="CaptionFontFamily" serializeAs="String">
              <value>Arial</value>
          </setting>
          <setting name="CaptionFontSize" serializeAs="String">
              <value>12</value>
          </setting>
          <setting name="CaptionFontColor" serializeAs="String">
              <value>#ffffffff</value>
          </setting>
          <setting name="BackGradientType" serializeAs="String">
              <value>Horizontal</value>
          </setting>
          <setting name="CallUpgrade" serializeAs="String">
              <value>True</value>
          </setting>
      </TAlex.BeautifulFractals.Properties.Settings>
  </userSettings>

  <applicationSettings>
    <TAlex.BeautifulFractals.Properties.Settings>
      <setting name="FractalsCollectionPath" serializeAs="String">
        <value>%localappdata%/T-Alex Software/Beautiful Fractals/Fractals.xml</value>
      </setting>
    </TAlex.BeautifulFractals.Properties.Settings>
  </applicationSettings>

  <system.diagnostics>
      <trace autoflush="true">
        <listeners>
          <add name="TextWriter" type="System.Diagnostics.TextWriterTraceListener" initializeData="C:\ProgramData\T-Alex Software\Beautiful Fractals\Trace.log" traceOutputOptions="ProcessId, DateTime, Callstack"/>
        </listeners>
      </trace>
    </system.diagnostics>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
